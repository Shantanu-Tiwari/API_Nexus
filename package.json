{
  "name": "apinexus",
  "private": true,
  "workspaces": [
    "apps/landing",
    "apps/dashboard"
  ],
  "scripts": {
    "dev:landing": "NEXT_DISABLE_ESLINT_PLUGIN=true npm --workspace=apps/landing run dev",
    "dev:dashboard": "NEXT_DISABLE_ESLINT_PLUGIN=true npm --workspace=apps/dashboard run dev",
    "dev": "concurrently \"npm run dev:landing\" \"npm run dev:dashboard\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
